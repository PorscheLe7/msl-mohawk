@model IEnumerable<MSL_APP.Models.ProductName>

@{
    ViewData["Title"] = "Index";
}

<h1>Product</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DownloadLink)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DownloadLink)
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm" asp-action="GetKey" asp-route-id="@item.Id">Get Key</a>
                </td>
            </tr>
        }
    </tbody>
</table>
